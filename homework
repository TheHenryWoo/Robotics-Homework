public abstract class subsystem {
   public String name;
  public int power;
  public abstract void init(); 
    public abstract void update();
  public subsystem (String name, int power){
    this.name = name; 
    this.power = power; 
  }
  public int init; 
  public String init(int power) {
    System.out.println("Init Sub");
  if(power == 1){
    String status = "active";
  }
  else {String status = "inactive";
    }
  }
  return status;
}
public String update;
public String update(int[] args){
  System.out.println("upadate Sub");
  if(power == 1){
    String status = "active";
    
  }
  else {
    String status = "inactive";
  }

}
  return status; 
}

